{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "!app/*",
    "!preload/*",
    "**/*.tsx",
    "preload/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "outDir": "./out_fe",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "frontend/components/*"
      ],
      "@contexts/*": [
        "frontend/contexts/*"
      ],
      "@renderer/*": [
        "frontend/*"
      ],
    }
  }
}
