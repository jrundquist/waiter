{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "**/*",
    "!app/*",
    "!preload/*",
    "preload/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "outDir": "./out_fe",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@contexts/*": [
        "contexts/*"
      ],
      "@windows/*": [
        "windows/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@screenFormatPlugin/*": [
        "screenFormatPlugin/*"
      ],
      "@styles": [
        "styles"
      ],
    }
  }
}
